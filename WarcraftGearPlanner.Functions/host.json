{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "serviceBus": {
      "messageHandlerOptions": { "maxConcurrentCalls": 5 },
      "sessionHandlerOptions": { "maxConcurrentSessions": 3 }
    }
  }
}